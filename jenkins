pipeline{
    agent any

    stages {
        stage('Checkout') {
            steps {
               git branch : 'main', url: 'https://github.com/1ms24mc038-Jeevitha/jenkinsfile'
            }
        }

        stage('Builds'){
            steps {
                 echo "Building.."
            }
        }

        stage('Test') {
            steps {
                echo "Testing..."
            }
        }
    }
}
